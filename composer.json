{
    "name": "ifedko/php-doctrine-dbal-pagination",
    "description": "Php pagination builder using doctrine dbal",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Ilya Fedko",
            "email": "fedko.ilya@gmail.com"
        },
        {
            "name": "Maxim Gnatenko",
            "email": "mgnatenko@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Ifedko\\DoctrineDbalPagination\\": ["src/"]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ifedko\\DoctrineDbalPagination\\Test\\": ["tests/"]
        }
    },
    "require": {
        "ext-pdo": "*",
        "php": "^7.4 || ^8.0",
        "doctrine/dbal": "^2.13 || ^3.8"
    },
    "require-dev": {
        "ext-json": "*",
        "mockery/mockery": "^1.6",
        "phpunit/phpunit": "^9.6",
        "symfony/phpunit-bridge": "^7.0",
        "symfony/doctrine-bridge": "^5.4 || ^6.4"
    }
}
